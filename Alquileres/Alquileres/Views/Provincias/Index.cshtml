@model IEnumerable<Alquileres.Models.Provincia>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Crear Nuevo", "Nuevo")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Detalle)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Detalle)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edita", new { id=item.ProvinciaId }) |
            @Html.ActionLink("Detalles", "Detalles", new { id=item.ProvinciaId }) |
            @Html.ActionLink("Eliminar", "Eliminar", new { id=item.ProvinciaId })
        </td>
    </tr>
}

</table>
